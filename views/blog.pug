extends layout

block content
  script(src="https://code.jquery.com/ui/1.12.0/jquery-ui.min.js", integrity="sha256-eGE6blurk5sHj+rmkfsGYeKyZx3M4bG+ZlFyA7Kns7E=",crossorigin="anonymous")
  div(id="posts")
    each post in posts
      div(class="post", alt="#{post.sauce}") 
        div(class="title") #{post.date}
        div(class="text")!= post.text
  script(src="/javascripts/randomize.js")
